{
  "version": 2,
  "projects": {
    "api": {
      "root": "apps/api",
      "sourceRoot": "apps/api/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/node:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/api",
            "main": "apps/api/src/main.ts",
            "tsConfig": "apps/api/tsconfig.app.json",
            "assets": ["apps/api/src/assets"]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/api/src/environments/environment.ts",
                  "with": "apps/api/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/node:execute",
          "options": {
            "buildTarget": "api:build"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/api/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/api"],
          "options": {
            "jestConfig": "apps/api/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "api-interfaces": {
      "root": "libs/api-interfaces",
      "sourceRoot": "libs/api-interfaces/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/api-interfaces/**/*.ts"]
          }
        }
      }
    },
    "ci": {
      "root": "apps/ci",
      "sourceRoot": "apps/ci/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/web:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/ci",
            "index": "apps/ci/src/index.html",
            "main": "apps/ci/src/main.tsx",
            "polyfills": "apps/ci/src/polyfills.ts",
            "tsConfig": "apps/ci/tsconfig.app.json",
            "assets": ["apps/ci/src/favicon.ico", "apps/ci/src/assets"],
            "styles": ["apps/ci/src/styles.scss"],
            "scripts": [],
            "webpackConfig": "@nrwl/react/plugins/webpack"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/ci/src/environments/environment.ts",
                  "with": "apps/ci/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/web:dev-server",
          "options": {
            "buildTarget": "ci:build",
            "hmr": true
          },
          "configurations": {
            "production": {
              "buildTarget": "ci:build:production",
              "hmr": false
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/ci/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/ci"],
          "options": {
            "jestConfig": "apps/ci/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "ci-e2e": {
      "root": "apps/ci-e2e",
      "sourceRoot": "apps/ci-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/ci-e2e/cypress.json",
            "tsConfig": "apps/ci-e2e/tsconfig.e2e.json",
            "devServerTarget": "ci:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "ci:serve:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/ci-e2e/**/*.{js,ts}"]
          }
        }
      }
    },
    "portfolio-static": {
      "root": "apps/portfolio-static",
      "sourceRoot": "apps/portfolio-static/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/web:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/portfolio-static",
            "index": "apps/portfolio-static/src/index.html",
            "main": "apps/portfolio-static/src/main.tsx",
            "polyfills": "apps/portfolio-static/src/polyfills.ts",
            "tsConfig": "apps/portfolio-static/tsconfig.app.json",
            "assets": [
              "apps/portfolio-static/src/favicon.ico",
              "apps/portfolio-static/src/assets"
            ],
            "styles": ["apps/portfolio-static/src/styles.scss"],
            "scripts": [],
            "webpackConfig": "@nrwl/react/plugins/webpack"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/portfolio-static/src/environments/environment.ts",
                  "with": "apps/portfolio-static/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/web:dev-server",
          "options": {
            "buildTarget": "portfolio-static:build",
            "hmr": true,
            "proxyConfig": "apps/portfolio-static/proxy.conf.json"
          },
          "configurations": {
            "production": {
              "buildTarget": "portfolio-static:build:production",
              "hmr": false
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/portfolio-static/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/portfolio-static"],
          "options": {
            "jestConfig": "apps/portfolio-static/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "portfolio-static-e2e": {
      "root": "apps/portfolio-static-e2e",
      "sourceRoot": "apps/portfolio-static-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/portfolio-static-e2e/cypress.json",
            "tsConfig": "apps/portfolio-static-e2e/tsconfig.e2e.json",
            "devServerTarget": "portfolio-static:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "portfolio-static:serve:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/portfolio-static-e2e/**/*.{js,ts}"]
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/react"
  },
  "generators": {
    "@nrwl/react": {
      "application": {
        "style": "scss",
        "linter": "eslint",
        "babel": true
      },
      "component": {
        "style": "scss"
      },
      "library": {
        "style": "scss",
        "linter": "eslint"
      }
    },
    "@nrwl/web:application": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/web:library": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest"
    }
  },
  "defaultProject": "portfolio-static"
}
