{
  "npmScope": "portfolio",
  "affected": {
    "defaultBase": "master"
  },
  "implicitDependencies": {
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    ".eslintrc.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": [
          "build",
          "lint",
          "test",
          "e2e",
          "build-storybook"
        ]
      }
    }
  },
  "targetDependencies": {
    "build": [
      {
        "target": "build",
        "projects": "dependencies"
      }
    ]
  },
  "projects": {
    "api": {
      "tags": []
    },
    "api-interfaces": {
      "tags": []
    },
    "ci": {
      "tags": []
    },
    "ci-e2e": {
      "tags": [],
      "implicitDependencies": ["ci"]
    },
    "portfolio-static": {
      "tags": []
    },
    "portfolio-static-e2e": {
      "tags": [],
      "implicitDependencies": ["portfolio-static"]
    }
  }
}
